<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="9.0dip" />
            <solid android:color="@color/ar4" />
            <stroke android:width="@dimen/a0" android:color="@color/l6" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="9.0dip" />
            <solid android:color="@color/ar4" />
            <stroke android:width="@dimen/a0" android:color="@color/l7" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="9.0dip" />
            <solid android:color="@color/ar4" />
            <stroke android:width="@dimen/a0" android:color="@color/l5" />
        </shape>
    </item>
</selector>
        <shape>
            <gradient android:startColor="@color/ar4" android:endColor="@color/ar4" android:angle="0.0" android:centerY="1.0" android:centerColor="@color/ar4" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/a_c" android:endColor="@color/a_c" android:angle="0.0" android:centerY="1.0" android:centerColor="@color/a_c" />
            </shape>
        </clip>
    </item>
</layer-list>
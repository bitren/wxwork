<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#ccebcd9a" />
            <stroke android:width="0.0dip" android:color="#ffd3d3d3" />
            <corners android:radius="15.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffebcd9a" />
            <stroke android:width="0.0dip" android:color="@color/atc" />
            <corners android:radius="15.0dip" />
        </shape>
    </item>
</selector>
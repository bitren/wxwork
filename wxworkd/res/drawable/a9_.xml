<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/y" />
            <solid android:color="@color/ln" />
            <stroke android:width="@dimen/a0" android:color="@color/la" />
        </shape>
    </item>
    <item android:state_selected="false" android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/y" />
            <solid android:color="@color/lp" />
            <stroke android:width="@dimen/a0" android:color="@color/lc" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/y" />
            <solid android:color="@color/lo" />
            <stroke android:width="@dimen/a0" android:color="@color/lb" />
        </shape>
    </item>
    <item android:state_enabled="false" android:state_selected="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <corners android:radius="@dimen/y" />
                    <solid android:color="@color/ln" />
                    <stroke android:width="@dimen/a0" android:color="@color/la" />
                </shape>
            </item>
            <item>
                <inset android:insetLeft="-4.0dip" android:insetRight="-4.0dip" android:insetTop="-4.0dip" android:insetBottom="1.0dip">
                    <shape android:shape="rectangle">
                        <padding />
                        <stroke android:width="3.0dip" android:color="@color/l7" />
                    </shape>
                </inset>
            </item>
        </layer-list>
    </item>
    <item android:state_selected="true" android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <corners android:radius="@dimen/y" />
                    <solid android:color="@color/lp" />
                    <stroke android:width="@dimen/a0" android:color="@color/lc" />
                </shape>
            </item>
            <item>
                <inset android:insetLeft="-4.0dip" android:insetRight="-4.0dip" android:insetTop="-4.0dip" android:insetBottom="1.0dip">
                    <shape android:shape="rectangle">
                        <padding />
                        <stroke android:width="3.0dip" android:color="@color/l7" />
                    </shape>
                </inset>
            </item>
        </layer-list>
    </item>
    <item android:state_selected="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <corners android:radius="@dimen/y" />
                    <solid android:color="@color/lo" />
                    <stroke android:width="@dimen/a0" android:color="@color/lb" />
                </shape>
            </item>
            <item>
                <inset android:insetLeft="-4.0dip" android:insetRight="-4.0dip" android:insetTop="-4.0dip" android:insetBottom="1.0dip">
                    <shape android:shape="rectangle">
                        <padding />
                        <stroke android:width="3.0dip" android:color="@color/l7" />
                    </shape>
                </inset>
            </item>
        </layer-list>
    </item>
</selector>
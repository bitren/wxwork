<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/y" />
            <solid android:color="@color/ac4" />
            <stroke android:width="@dimen/a0" android:color="@color/ac7" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/y" />
            <solid android:color="@color/ac6" />
            <stroke android:width="@dimen/a0" android:color="@color/ac9" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/y" />
            <solid android:color="@color/ac5" />
            <stroke android:width="@dimen/a0" android:color="@color/ac8" />
        </shape>
    </item>
</selector>
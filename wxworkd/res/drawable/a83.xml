<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#5945709a" />
            <stroke android:width="0.0dip" android:color="#5945709a" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#cc45709a" />
            <stroke android:width="0.0dip" android:color="#ff45709a" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ff45709a" />
            <stroke android:width="0.0dip" android:color="#ff45709a" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
</selector>